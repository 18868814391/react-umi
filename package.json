{
  "name": "admin-antd-react",
  "version": "1.0.0",
  "private": true,
  "description": "React Ant Design Umi Admin template",
  "scripts": {
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "svgo": "svgo -f src/assets/iconsvg --config=src/assets/iconsvg/svgo.yml"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^5.0.19",
    "@ckeditor/ckeditor5-build-decoupled-document": "^23.1.0",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@toast-ui/react-editor": "^2.5.3",
    "@uiw/react-amap": "^2.2.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "~3.3.11",
    "antd": "^4.16.11",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "echarts": "^4.9.0",
    "lint-staged": "^10.5.4",
    "localforage": "^1.9.0",
    "lodash.debounce": "^4.0.8",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.14.0",
    "umi": "~3.3.11",
    "umi-plugin-keep-alive": "^0.0.1-beta.29",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.10",
    "@types/lodash.debounce": "^4.0.6",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@umijs/fabric": "^2.6.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-plugin-react": "^7.25.1",
    "express": "^4.17.1",
    "less-vars-to-js": "^1.3.0",
    "mockjs": "^1.1.0",
    "svg-sprite-loader": "^5.2.1",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.2",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "react",
    "typescript",
    "admin",
    "template",
    "antd",
    "Ant Design",
    "umi"
  ]
}
